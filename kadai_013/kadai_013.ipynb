      "source": [
        "# 消費税を加えた金額を計算する関数\n",
        "def calc_tax(price, tax_rate):\n",
        "    # 税率を％表記（例：10）から小数に直して計算\n",
        "    total = price * (1 + tax_rate / 100)\n",
        "    # 計算結果を戻り値として返す\n",
        "    return total\n",
        "\n",
        "# 関数を呼び出して計算\n",
        "result = calc_tax(110, 10)\n",
        "\n",
        "# 結果を出力\n",
        "print(result)\n"
      ]
    }
  ]
